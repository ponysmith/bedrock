{
  "name": "bedrock",
  "version": "1.0.0",
  "description": "Bedrock",
  "main": "index.js",
  "scripts": {
    "start": "npm run docs:clean && concurrently --kill-others \"npm run docs:watch\" \"npm run docs:server\" \"npm run build:dev\"",
    "docs:clean": "hexo clean --cwd docs",
    "docs:server": "hexo server --static --cwd docs",
    "docs:watch": "hexo generate --watch --cwd docs",
    "build:dev": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "build": "npm run build:prod"
  },
  "author": "Pony Smith",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.0",
    "gulp": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^7.0.0",
    "node-yaml": "^3.2.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "reselect": "^4.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "acorn": "^6.1.1"
  }
}
